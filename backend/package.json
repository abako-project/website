{
  "name": "backend",
  "version": "1.0.0",
  "scripts": {
    "start2": "PORT=3001 node ./bin/www",
    "start": "PORT=3001 supervisor ./bin/www",
    "dev": "cross-env NODE_ENV=development PORT=3001 nodemon ./bin/www",
