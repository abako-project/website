    "express-session": "^1.17.3",
    "method-override": "^3.0.0",
    "multer": "^2.0.1",
    "node-localstorage": "^3.0.5",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "sass": "^1.89.2",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.3",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.1.6",
